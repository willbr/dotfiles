function j {
    if [ -z "$1" ]; then
        cd ~; echo ~
        return
    else
        local target=`jumpto jump $1`
        if [ -n "$target" ]; then
            if [ -d "$target" ]; then
                cd $target; echo $target
            else
                echo error: $target is not a directory >&2
            fi
        else
            echo alias not found
        fi
    fi
}

alias jls='jumpto list'
alias ja='jumpto add'
alias je='jumpto edit'
alias jb='cd -'

